Kripke "kripke.dot" Space "space.dot" Eval "eval.csv";

Let green = [green] & (![black]);
Let black = [black];
Let white = (![green]) & (![black]);
Let red = [red];
Let brown = [brown];
Let danger = [danger];
Let blue = [blue];
Let cyan = [cyan];
Let purple = [purple];
Let yellow = [yellow];

Let obstacle = black | brown | danger;
Let agent = blue | cyan | purple | yellow;
Let safe = white & (!obstacle);

Let Phi1 = safe S obstacle;

Let R(a,b) = !((!b) S (!a));
Let T(a,b) = a & (!((!b) S(! (a |b)))); // NB: there is an infinite loop here if T is defined using R. Check.
 
Let Phi2 = T(safe,(red & safe));
Let Phi3 = T(safe,green);
Let Phi4 = (red & safe) & (T(safe,green));
Let Phi5 = T(safe,Phi4);

Check "0x010101" black;
Check "0x00FF00" green;
Check "0xFFFFFF" white;
Check "0xFF0000" red;
Check "0x663300" brown;
Check "0xFF6600" danger;
Check "0x0000FF" blue;
Check "0x00FFFF" cyan;
Check "0x800080" purple;
Check "0xFFF600" yellow;

